package com.all{		import flash.display.Sprite;	import flash.display.Graphics;	import flash.events.MouseEvent;			public class LineGraph extends Sprite {				//private var _lists:selectedNodesList;		private var _lists:Vector.<Node>;		public function LineGraph(lists:Vector.<Node>) {			_lists = lists;		}				public function drawBudgetGraph(){						this.graphics.lineStyle(2);			//***********compression needed to fit into the display***********//			this.graphics.beginFill(0x66CC99); 					this.graphics.moveTo(100,_list);			this.graphics.lineTo(225,_lists[1].cost);			this.graphics.drawCircle(100,_lists[0].cost,10); // (x spacing, y spacing, width, height)			this.graphics.beginFill(0x330033); 			this.graphics.moveTo(225,_lists[1].cost);			this.graphics.lineTo(350,_lists[2].cost);			this.graphics.drawCircle(225,_lists[1].cost,10);			this.graphics.beginFill(0xFFFF33); 					this.graphics.moveTo(350,_lists[2].cost);			this.graphics.lineTo(480,_lists[3].cost);			this.graphics.drawCircle(350,_lists[2].cost,10);			this.graphics.beginFill(0x3399FF); 			this.graphics.drawCircle(480,_lists[3].cost,10);					}			}	}