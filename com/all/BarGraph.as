package com.all {		import flash.display.Sprite;	import flash.display.Graphics;		public class BarGraph extends Sprite {		private var _lists:Vector.<Node>;		private var xpos = 50;	    private var ypos = 350;		private var barwidth = 80;		private var barheight = 350;		private var barspace = 10;		public function BarGraph(lists:Vector.<Node>) {			_lists = lists;		}		public function drawBudgetGraph(){			//vertical axis			this.graphics.lineStyle(3);			this.graphics.moveTo(xpos, 50);			this.graphics.lineTo(xpos,350);						//horizontal axis			this.graphics.lineStyle(3);			this.graphics.moveTo(xpos,350);			this.graphics.lineTo(500,350);			this.graphics.beginFill(0xFF0000); 			//***********compression needed to fit into the display***********//			this.graphics.drawRect(xpos+barspace,barheight-datas, barwidth,_lists[0].cost); // (x spacing, y spacing, width, height)			this.graphics.drawRect((xpos+barwidth+2*barspace),barheight-datas, barwidth,_lists[1].cost); 			this.graphics.drawRect((xpos+2*barwidth)+3*barspace,barheight-datas, barwidth,_lists[2].cost); // (x spacing, y spacing, width, height)			this.graphics.drawRect((xpos+3*barwidth)+4*barspace,barheight-datas, barwidth,_lists[3].cost); // (x spacing, y spacing, width, height)			this.graphics.endFill();		}	}	}
